AnnotationAssertion(ms:classDescription aci:Riscontro_stato_giuridico_originario "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un <a href=\"/predicate/concept/aci:Riscontro_stato_giuridico_originario\"><span style=\" text-decoration: underline; \">riscontro dello stato giuridico originario</span></a>  è un <a href=\"/predicate/concept/aci:Formalita_di_variazione\"><span style=\" text-decoration: underline; \">formalità di variazione</span></a>  con<a href=\"/predicate/concept/aci:Finalita_riscontro_stato_giuridico_originario\"><span style=\" text-decoration: underline; \">finalità di_riscontro_dello stato_giuridico_originario</span></a>.</p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Riscontro_stato_giuridico_originario ObjectSomeValuesFrom(aci:richiesta_per_finalita aci:Finalita_riscontro_stato_giuridico_originario))
AnnotationAssertion(ms:classDescription aci:Riscossione_iniziale "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Riscossione\"><span style=\" text-decoration: underline; \">riscossione</span></a>  è <a href=\"/predicate/concept/aci:Riscossione_iniziale\"><span style=\" text-decoration: underline; \">iniziale</span></a>  se il <a href=\"/predicate/role/aci:paga_a_riscossore\"><span style=\" text-decoration: underline; \">pagante</span></a>  della riscossione corrisponde al <a href=\"/predicate/role/aci:ha_pagante\"><span style=\" text-decoration: underline; \">pagante</span></a>  del <a href=\"/predicate/concept/aci:Pagamento\"><span style=\" text-decoration: underline; \">pagamento</span></a>  di cui la riscossione <a href=\"/predicate/role/aci:ha_riscossione\"><span style=\" text-decoration: underline; \">è parte</span></a> . Questo in generale significa che la riscossione iniziale è il primo della serie di trasferimenti che realizza il pagamento.    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Riscossione_somme_prenotate_a_debito "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Riscossione_somme_prenotate_a_debito\"><span style=\" text-decoration: underline; \">riscossione somme prenotate a debito</span></a>  è una <a href=\"/predicate/concept/aci:Formalita\"><span style=\" text-decoration: underline; \">formalità</span></a>  (<a href=\"/predicate/concept/aci:Formalita_non_sulla_rappresentazione\"><span style=\" text-decoration: underline; \">non sulla rappresentazione</span></a> ) avente per <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  una <a href=\"/predicate/concept/aci:Finalita_riscossione_somme_prenotate_a_debito\"><span style=\" text-decoration: underline; \">riscossione di somme prenotate a debito</span></a>     </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Riscossione_somme_prenotate_a_debito ObjectSomeValuesFrom(aci:richiesta_per_finalita aci:Finalita_riscossione_somme_prenotate_a_debito))
AnnotationAssertion(ms:classDescription aci:Scrittura_privata "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Scrittura_privata\"><span style=\" text-decoration: underline; \">scrittura privata</span></a> , nel diritto italiano, è il documento redatto per iscritto (con qualunque mezzo: manuale, meccanico, elettronico) e sottoscritto con firma autografa da soggetto che, in virtù della sottoscrizione, prende il nome di autore.La struttura della <a href=\"/predicate/concept/aci:Scrittura_privata\"><span style=\" text-decoration: underline; \">scrittura privata</span></a>  si compone di vari elementi tra cui il corpo, la sottoscrizione e il testo. Il documento, inoltre, deve soddisfare i requisiti di privatezza, autenticità e genuinità.    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Semirimorchio "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un <a href=\"/predicate/concept/aci:Semirimorchio\"><span style=\" text-decoration: underline; \">semirimorchio</span></a> è un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a> <a href=\"/predicate/concept/aci:Veicolo_a_trazione_di_veicolo_a_motore\"><span style=\" text-decoration: underline; \">destinato al traino</span></a> da parte di un <a href=\"/predicate/concept/aci:Trattore_stradale\"><span style=\" text-decoration: underline; \">trattore stradale</span></a> costruito in modo tale che una parte di esso si sovrapponga all'unità motrice e che una parte notevole della sua massa o del suo carico sia sopportata da detta motrice. </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Sentenza "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Sentenza\"><span style=\" text-decoration: underline; \">sentenza</span></a>  è un <a href=\"/predicate/concept/aci:Atto\"><span style=\" text-decoration: underline; \">atto</span></a>  attraverso il quale un giudice definisce in tutto o in parte la controversia che gli è stata sottoposta, risolvendo le questioni in fatto ed in diritto proposte dalle parti.    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Sgravio_totale_per_indebito "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Uno <a href=\"/predicate/concept/aci:Sgravio_totale_per_indebito\"><span style=\" text-decoration: underline; \">sgravio totale per indebito</span></a>  è un <a href=\"/predicate/concept/aci:Provvedimento\"><span style=\" text-decoration: underline; \">provvedimento</span></a>  (<a href=\"/predicate/concept/aci:Provvedimento_giudiziale\"><span style=\" text-decoration: underline; \">giudiziale</span></a>  e <a href=\"/predicate/concept/aci:Provvedimento_di_cancellazione\"><span style=\" text-decoration: underline; \">di_cancellazione</span></a> ) è il riconoscimento che un  <a href=\"/predicate/concept/aci:Provvedimento_di_fermo_amministrativo\"><span style=\" text-decoration: underline; \">provvedimento di fermo amministrativo</span></a>  è stato iscritto erroneamente, perché basato su una somma non dovuta dal contribuente. In caso di sgravio, il concessionario della riscossione provvederà a richiedere al PRA la cancellazione gratuita dell'iscrizione del fermo.    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Soggetto "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un <a href=\"/predicate/concept/aci:Soggetto\"><span style=\" text-decoration: underline; \">soggetto</span></a>  è una qualunque entità che gioca un qualsivoglia ruolo nell'ambito della fiscalità dell'auto. Un <a href=\"/predicate/concept/aci:Soggetto\"><span style=\" text-decoration: underline; \">soggetto</span></a>  può essere una <a href=\"/predicate/concept/aci:Persona\"><span style=\" text-decoration: underline; \">persona</span></a>  (<a href=\"/predicate/concept/aci:Persona_fisica\"><span style=\" text-decoration: underline; \">fisica</span></a>  o <a href=\"/predicate/concept/aci:Persona_giuridica\"><span style=\" text-decoration: underline; \">giuridica</span></a> ), oppure un'<a href=\"/predicate/concept/aci:Organizzazione\"><span style=\" text-decoration: underline; \">organizzazione</span></a> .    </p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Soggetto ObjectSomeValuesFrom(aci:ha_stato_di_soggetto owl:Thing))
SubClassOf(aci:Soggetto aci:Entita_avente_stati)
AnnotationAssertion(ms:classDescription aci:Sospensione "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Sospensione\"><span style=\" text-decoration: underline; \">sospensione</span></a>  è un <a href=\"/predicate/concept/aci:Periodo_copertura_RCA\"><span style=\" text-decoration: underline; \">periodo di copertura RCA</span></a>  una parte del quale, su esplicita richiesta dell'<a href=\"/predicate/concept/aci:Intestazione\"><span style=\" text-decoration: underline; \">intestatario</span></a> , è privata dalla copertura in quanto il <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  assicurato è previsto non circolante. Si noti quanto segue:</p>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">la sospensione è un diritto dell'intestatario e può durare da 3 mesi ad un anno;</li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">il premio Rc auto già pagato viene congelato e riprende ad essere scalato quando la polizza viene riattivata;</li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">durante il periodo di sospensione il veicolo non può per nessun motivo circolare su strada o anche solo essere parcheggiato in aree pubbliche;</li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">nel caso di violazione, la multa è la stessa che si avrebbe nel caso di circolazione con veicolo non assicurato;</li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">infine, la polizza non può essere sospesa se il veicolo assicurato è in leasing o in finanziamento.    </li></ul></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Sospensione ObjectSomeValuesFrom(ObjectInverseOf(aci:ha_esenzione_per) owl:Thing))
EquivalentClasses(aci:Sospensione DataSomeValuesFrom(aci:data_inizio_sospensione rdfs:Literal))
EquivalentClasses(aci:Sospensione DataSomeValuesFrom(aci:motivazione_sospensione rdfs:Literal))
AnnotationAssertion(ms:classDescription aci:Sospensione_giacenza_rivenditore "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Sospensione_giacenza_rivenditore\"><span style=\" text-decoration: underline; \">sospensione per giacenza presso un rivenditore</span></a> è una particolare <a href=\"/predicate/concept/aci:Sospensione\"><span style=\" text-decoration: underline; \">sospensione</span></a> dal pagamento del <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto\"><span style=\" text-decoration: underline; \">bollo</span></a> che si applica nei periodi tributari in cui il veicolo è in giacenza presso un rivenditore.</p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Sospensione_giacenza_rivenditore aci:Sospensione)
AnnotationAssertion(ms:classDescription aci:Stato "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">L'ontologia ha un certo numero di concetti &quot;temporizzati&quot;. Questi classificano oggetti per come essi sono osservabili in diversi istanti temporali. L'intervallo di tempo in cui le proprietà dell'oggetto che sono di interesse per l'ontologia non cambiano è denotato tramite uno <a href=\"/predicate/concept/aci:Stato\"><span style=\" text-decoration: underline; \">stato.</span></a> Gli istanti in cui una o più proprietà dell'oggetto cambiano, sono i tempi di discontinuità e definiscono gli estremi di uno stato, o in altri termini il suo <a href=\"/predicate/attribute/aci:inizio\"><span style=\" text-decoration: underline; \">inizio</span></a> e la sua <a href=\"/predicate/attribute/aci:fine\"><span style=\" text-decoration: underline; \">fine</span></a>. Due stati contigui nel tempo avranno la <a href=\"/predicate/attribute/aci:fine\"><span style=\" text-decoration: underline; \">fine</span></a> di uno pari all'<a href=\"/predicate/attribute/aci:inizio\"><span style=\" text-decoration: underline; \">inizio</span></a> dell'altro. Nell'ontologia si è deciso di introdurre uno stato corrispondente ad un oggetto ogni qual volta cambia il valore di una sua proprietà rilevante.</p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">  </p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"> A titolo di esempio si consideri una persona. La sua data di nascita è una proprietà che non varia nel tempo, ed è pertanto considerata una proprietà rigida, e quindi sarà associata direttamente alla persona. Il suo indirizzo di residenza è invece una proprietà che può variare nel tempo, e pertanto sarà associata ai suoi stati. I punti di discontinuità tra gli stati rappresentano i momenti in cui la persona ha cambiato residenza.</p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Stato ObjectSomeValuesFrom(ObjectInverseOf(aci:ha_stato) owl:Thing))
EquivalentClasses(aci:Stato DataSomeValuesFrom(aci:inizio rdfs:Literal))
SubClassOf(aci:Stato aci:Entita_non_avente_stati)
AnnotationAssertion(ms:classDescription aci:Stato_con_fermo_amministrativo "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Uno <a href=\"/predicate/concept/aci:Stato_con_fermo_amministrativo\"><span style=\" text-decoration: underline; \">stato con fermo amministrativo</span></a>  è uno <a href=\"/predicate/concept/aci:Stato_con_vincoli_o_gravami\"><span style=\" text-decoration: underline; \">stato con vincoli o gravami</span></a>  che fa seguito ad un <a href=\"/predicate/concept/aci:Provvedimento_di_fermo_amministrativo\"><span style=\" text-decoration: underline; \">fermo amministrativo</span></a>     </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Stato_con_fermo_amministrativo aci:Stato_con_sospensione_dalla_circolazione)
AnnotationAssertion(ms:classDescription aci:Stato_con_ipoteca "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Il concetto S<a href=\"/predicate/concept/aci:Stato_con_ipoteca\"><span style=\" text-decoration: underline; \">tato_con_ipoteca</span></a>  specializza il concetto <a href=\"/predicate/concept/aci:Stato_con_ipoteca_o_privilegio\"><span style=\" text-decoration: underline; \">Stato_con_ipoteca_o_privilegio</span></a> e denota fra gli stati su cui grava una ipoteca o un privilegio.</p></OntologyDescription>"^^xsd:string)
DisjointClasses(aci:Stato_con_ipoteca aci:Stato_con_privilegio)
AnnotationAssertion(ms:classDescription aci:Stato_con_ipoteca_o_privilegio "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Uno <a href=\"/predicate/concept/aci:Stato_con_ipoteca_o_privilegio\"><span style=\" text-decoration: underline; \">stato con ipoteca o privilegio</span></a>  è uno <a href=\"/predicate/concept/aci:Stato_con_vincoli_o_gravami\"><span style=\" text-decoration: underline; \">stato con vincoli o gravami</span></a>  nel quale si trova un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a> su cui è <a href=\"/predicate/concept/aci:Iscrizione_ipoteca_o_privilegio\"><span style=\" text-decoration: underline; \">registrata una ipoteca o privilegio</span></a>, (ma che non necessariamente è anche soggetto ad un ulteriore <a href=\"/predicate/concept/aci:Stato_con_vincoli\"><span style=\" text-decoration: underline; \">vincolo</span></a>) e che quindi nello stato in questione risulta dato in garanzia di un credito altrui.    </p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Stato_con_ipoteca_o_privilegio ObjectUnionOf(aci:Stato_con_ipoteca aci:Stato_con_privilegio))
EquivalentClasses(aci:Stato_con_ipoteca_o_privilegio DataSomeValuesFrom(aci:tipo_credito rdfs:Literal))
AnnotationAssertion(ms:classDescription aci:Stato_con_obbligo_di_registrazione_al_PRA "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Uno <a href=\"/predicate/concept/aci:Stato_con_obbligo_di_registrazione_al_PRA\"><span style=\" text-decoration: underline; \">stato con obbligo di registrazione al PRA</span></a>  è uno stato di un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a> per il quale vi è l'obbligo di registrazione al Pubblico Registro Automobilistico (PRA).</p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Stato_con_obbligo_di_registrazione_al_PRA aci:Stato_di_veicolo)
SubClassOf(aci:Stato_con_obbligo_di_registrazione_al_PRA ObjectAllValuesFrom(ObjectInverseOf(aci:ha_stato_di_veicolo) aci:Veicolo_registrabile_al_PRA))
AnnotationAssertion(ms:classDescription aci:Stato_con_perdita_di_possesso "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Lo <a href=\"/predicate/concept/aci:Stato_con_perdita_di_possesso\"><span style=\" text-decoration: underline; \">stato con perdita di possesso</span></a>  è uno <a href=\"/predicate/concept/aci:Stato_di_veicolo\"><span style=\" text-decoration: underline; \">stato di veicolo</span></a> che segue la <a href=\"/predicate/concept/aci:Finalita_perdita_di_possesso\"><span style=\" text-decoration: underline; \">registrazione della perdita del possesso</span></a> del veicolo.</p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Stato_con_perdita_di_possesso aci:Stato_di_veicolo)
AnnotationAssertion(ms:classDescription aci:Stato_con_pignoramento "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Uno <a href=\"/predicate/concept/aci:Stato_con_pignoramento\"><span style=\" text-decoration: underline; \">stato con pignoramento</span></a>  è uno <a href=\"/predicate/concept/aci:Stato_con_vincoli_o_gravami\"><span style=\" text-decoration: underline; \">stato con vincoli o gravami</span></a>  che fa seguito ad un <a href=\"/predicate/concept/aci:Autorizzazione_al_pignoramento\"><span style=\" text-decoration: underline; \">pignoramento</span></a> .    </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Stato_con_pignoramento aci:Stato_con_sospensione_dalla_circolazione)
AnnotationAssertion(ms:classDescription aci:Stato_con_privilegio "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Il concetto <a href=\"/predicate/concept/aci:Stato_con_privilegio\"><span style=\" text-decoration: underline; \">Stato con privilegio</span></a>  specializza il concetto <a href=\"/predicate/concept/aci:Stato_con_ipoteca_o_privilegio\"><span style=\" text-decoration: underline; \">Stato_con_ipoteca_o_privilegio</span></a> e denota fra gli stati con ipoteca o privilegio quelli su cui è <a href=\"/predicate/concept/aci:Finalita_iscrizione_privilegio\"><span style=\" text-decoration: underline; \">registrato un privilegio</span></a>. Con l’entrata in vigore del codice civile, i privilegi iscritti sugli autoveicoli sono stati considerati come ipoteche.</p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Stato_con_registrazione_al_PRA "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Uno <a href=\"/predicate/concept/aci:Stato_di_veicolo\"><span style=\" text-decoration: underline; \">stato di veicolo</span></a> è <a href=\"/predicate/concept/aci:Stato_con_registrazione_al_PRA\"><span style=\" text-decoration: underline; \">con registrazione al PRA</span></a> quando è stato assolto l'obbligo di comunicazione al PRA delle caratteristiche del veicolo in quello stato.    </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Stato_con_registrazione_al_PRA aci:Stato_di_veicolo)
SubClassOf(aci:Stato_con_registrazione_al_PRA ObjectSomeValuesFrom(aci:stato_comunicato_a ObjectOneOf(aci:ACI)))
SubClassOf(aci:Stato_con_registrazione_al_PRA ObjectAllValuesFrom(ObjectInverseOf(aci:ha_stato_di_veicolo) aci:Veicolo_registrabile_al_PRA))
DisjointClasses(aci:Stato_con_registrazione_al_PRA aci:Stato_rappresentato)
AnnotationAssertion(ms:classDescription aci:Stato_con_sospensione_dalla_circolazione "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Uno <a href=\"/predicate/concept/aci:Stato_con_sospensione_dalla_circolazione\"><span style=\" text-decoration: underline; \">stato con sospensione della carta di circolazione</span></a>  è uno <a href=\"/predicate/concept/aci:Stato_di_veicolo\"><span style=\" text-decoration: underline; \">stato di veicolo</span></a>  che si caratterizza per la sospensione della carta di circolazione del veicolo stesso.    </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Stato_con_sospensione_dalla_circolazione aci:Stato_di_veicolo)
AnnotationAssertion(ms:classDescription aci:Stato_con_vincoli "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Il concetto <a href=\"/predicate/concept/aci:Stato_con_vincoli\"><span style=\" text-decoration: underline; \">Stato_con_vincoli</span></a> specializza il concetto <a href=\"/predicate/concept/aci:Stato_con_vincoli_o_gravami\"><span style=\" text-decoration: underline; \">Stato_con_vincoli_o_gravami</span></a>  e denota gli stati fra questi in cui il  <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  è soggetto ad un vincolo (ad esempio, fermo amministrativo, sequestro, pignoramento, etc.), ma non necessariamente anche ad ipoteca o privilegio.   </p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Stato_con_vincoli ObjectUnionOf(aci:Stato_con_fermo_amministrativo aci:Stato_con_pignoramento aci:Stato_derivato_da_congelamento_beni aci:Stato_derivato_da_sequestro aci:Stato_di_veicolo_in_concordato_preventivo aci:Stato_di_veicolo_in_massa_fallimentare))
AnnotationAssertion(ms:classDescription aci:Stato_con_vincoli_o_gravami "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Uno <a href=\"/predicate/concept/aci:Stato_con_vincoli_o_gravami\"><span style=\" text-decoration: underline; \">stato con vincoli o gravami</span></a>  è uno <a href=\"/predicate/concept/aci:Stato_di_veicolo\"><span style=\" text-decoration: underline; \">stato</span></a>  di un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  sul quale pendono, in quello stato, specifici vincoli o gravami (ad esempio, ipoteche, fermi amministrativi, etc.).    </p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Stato_con_vincoli_o_gravami ObjectUnionOf(aci:Stato_con_ipoteca_o_privilegio aci:Stato_con_vincoli))
EquivalentClasses(aci:Stato_con_vincoli_o_gravami ObjectSomeValuesFrom(aci:ha_vincolo_o_gravame owl:Thing))
SubClassOf(aci:Stato_con_vincoli_o_gravami aci:Stato_di_veicolo)
AnnotationAssertion(ms:classDescription aci:Stato_corrente "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Uno <a href=\"/predicate/concept/aci:Stato_corrente\"><span style=\" text-decoration: underline; \">stato corrente</span></a> è uno <a href=\"/predicate/concept/aci:Stato\"><span style=\" text-decoration: underline; \">stato</span></a> attuale, ossia lo stato in cui il periodo di validità non è al presente terminato. Per questo motivo uno stato corrente non ha una data di fine.    </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Stato_corrente aci:Stato)
