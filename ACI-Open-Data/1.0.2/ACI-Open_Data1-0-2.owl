AnnotationAssertion(ms:classDescription aci:Rappresentazione_nel_PRA "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Uno <a href=\"/predicate/concept/aci:Stato_rappresentato_dal_PRA\"><span style=\" text-decoration: underline; \">stato rappresentato dal PRA</span></a>  è uno <a href=\"/predicate/concept/aci:Stato_rappresentato\"><span style=\" text-decoration: underline; \">stato rappresentato</span></a>  tale che la <a href=\"/predicate/role/aci:est_rappresentato_da\"><span style=\" text-decoration: underline; \">rappresentazione</span></a> è gestita dal PRA.    </p></OntologyDescription>"^^xsd:string)
