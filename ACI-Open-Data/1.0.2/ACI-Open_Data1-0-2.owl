AnnotationAssertion(ms:classDescription aci:Cessione_o_proroga_di_usufrutto "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Cessione_o_proroga_di_usufrutto\"><span style=\" text-decoration: underline; \">cessione o proroga di usufrutto</span></a>  è una <a href=\"/predicate/concept/aci:Formalita\"><span style=\" text-decoration: underline; \">formalità</span></a>  (<a href=\"/predicate/concept/aci:Formalita_di_variazione\"><span style=\" text-decoration: underline; \">di variazione</span></a> ) avente per <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  una delle seguenti:</p>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">la <a href=\"/predicate/concept/aci:Finalita_proroga_di_usufrutto\"><span style=\" text-decoration: underline; \">proroga di un usufrutto</span></a> </li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">la <a href=\"/predicate/concept/aci:Finalita_cessione_di_usufrutto\"><span style=\" text-decoration: underline; \">cessione di un usufrutto</span></a>     </li></ul></OntologyDescription>"^^xsd:string)
