AnnotationAssertion(ms:classDescription aci:Stato_derivato_da_congelamento_beni "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Uno <a href=\"/predicate/concept/aci:Stato_derivato_da_congelamento_beni\"><span style=\" text-decoration: underline; \">stato derivato da congelamento di beni</span></a>  è uno <a href=\"/predicate/concept/aci:Stato_con_vincoli_o_gravami\"><span style=\" text-decoration: underline; \">stato con vincoli o gravami</span></a>  che fa seguito ad un <a href=\"/predicate/concept/aci:Autorizzazione_al_congelamento_dei_beni\"><span style=\" text-decoration: underline; \">congelamento di beni</span></a> .    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Stato_derivato_da_sequestro "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Uno <a href=\"/predicate/concept/aci:Stato_derivato_da_sequestro\"><span style=\" text-decoration: underline; \">stato derivato da sequestro</span></a>  è uno <a href=\"/predicate/concept/aci:Stato_con_vincoli_o_gravami\"><span style=\" text-decoration: underline; \">stato con vincoli o gravami</span></a>  che fa seguito ad un <a href=\"/predicate/concept/aci:Autorizzazione_al_sequestro\"><span style=\" text-decoration: underline; \">sequestro</span></a> .    </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Stato_derivato_da_sequestro aci:Stato_con_sospensione_dalla_circolazione)
