AnnotationAssertion(ms:classDescription aci:Costituzione_di_usufrutto "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Costituzione_di_usufrutto\"><span style=\" text-decoration: underline; \">costituzione di usufrutto</span></a>  è una <a href=\"/predicate/concept/aci:Formalita\"><span style=\" text-decoration: underline; \">formalità</span></a>  (<a href=\"/predicate/concept/aci:Formalita_di_variazione\"><span style=\" text-decoration: underline; \">di variazione</span></a> ) avente per <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  la <a href=\"/predicate/concept/aci:Finalita_costituzione_di_usufrutto\"><span style=\" text-decoration: underline; \">costituzione di un usufrutto</span></a>     </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Costituzione_di_usufrutto ObjectSomeValuesFrom(aci:richiesta_per_finalita aci:Finalita_costituzione_di_usufrutto))
AnnotationAssertion(ms:classDescription aci:Denotazione_di_luogo "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Denotazione_di_luogo\"><span style=\" text-decoration: underline; \">denotazione di luogo</span></a>  è un significante linguistico che <a href=\"/predicate/role/aci:ha_denotazione\"><span style=\" text-decoration: underline; \">si riferisce</span></a>  ad un <a href=\"/predicate/concept/aci:Stato_di_luogo\"><span style=\" text-decoration: underline; \">luogo in un suo stato</span></a> . Essa può essere considerata </p>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">semplice descrizione: un segno o un oggetto linguistico che non ha senso o non interessa scomporre ulteriormente, oppure </li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">complessa: una formula contenente sub-formule; </li></ul>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:1; text-indent:0px;\">l'ontologia prevede le seguenti sub-formule: il <a href=\"/predicate/attribute/aci:CAP\"><span style=\" text-decoration: underline; \">CAP</span></a> , il <a href=\"/predicate/attribute/aci:toponimo\"><span style=\" text-decoration: underline; \">toponimo</span></a> , e il selettore (<a href=\"/predicate/attribute/aci:numero_civico\"><span style=\" text-decoration: underline; \">il numero civico</span></a> oppure <a href=\"/predicate/attribute/aci:indicatore_km\"><span style=\" text-decoration: underline; \">l'indicatore chilometrico</span></a>).</p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:1; text-indent:0px;\"><br /></p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Nel secondo caso può specializzare altre denotazioni di luogo, nel caso queste ultime si riferiscano a luoghi dei quali il luogo denotato <a href=\"/predicate/role/aci:partecipa_a_luogo\"><span style=\" text-decoration: underline; \">è parte</span></a> . Questi luoghi sono identificati in parti proprie della denotazione complessa.</p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Denotazione_di_luogo ObjectSomeValuesFrom(ObjectInverseOf(aci:ha_denotazione) owl:Thing))
SubClassOf(aci:Denotazione_di_luogo aci:Entita_non_avente_stati)
AnnotationAssertion(ms:classDescription aci:Denotazione_di_luogo_estero "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Denotazione_di_luogo_estero\"><span style=\" text-decoration: underline; \">denotazione di luogo estero</span></a> è una <a href=\"/predicate/concept/aci:Denotazione_di_luogo\"><span style=\" text-decoration: underline; \">denotazione</span></a> di un <a href=\"/predicate/concept/aci:Luogo\"><span style=\" text-decoration: underline; \">luogo</span></a> che si trova al di fuori dei confini del territorio italiano. Le sub-formule di una <a href=\"/predicate/concept/aci:Denotazione_di_luogo_estero\"><span style=\" text-decoration: underline; \">denotazione di luogo estero</span></a> sono il codice ZIP e l'<a href=\"/predicate/attribute/aci:indirizzo_estero\"><span style=\" text-decoration: underline; \">indirizzo estero</span></a>.   </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Denotazione_di_luogo_estero aci:Denotazione_di_luogo)
DisjointClasses(aci:Denotazione_di_luogo_estero DataSomeValuesFrom(aci:CAP rdfs:Literal))
AnnotationAssertion(ms:classDescription aci:Denotazione_indirizzo_di_residenza_in_applicazione_di_bollo_auto "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una istanza di questo concetto è una <a href=\"/predicate/concept/aci:Denotazione_di_luogo\"><span style=\" text-decoration: underline; \">denotazione di luogo</span></a> specifica per gli <a href=\"/predicate/concept/aci:Stato_di_soggetto_rappresentato_in_applicazione_di_bollo_auto\"><span style=\" text-decoration: underline; \">stati di soggetto rappresentati in una applicazione di bollo auto</span></a>.</p></OntologyDescription>"^^xsd:string)
DisjointClasses(aci:Denotazione_indirizzo_di_residenza_in_applicazione_di_bollo_auto aci:Denotazione_indirizzo_per_soggetto_in_veicolo)
AnnotationAssertion(ms:classDescription aci:Denotazione_indirizzo_per_soggetto_in_veicolo "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una istanza di questo concetto è una <a href=\"/predicate/concept/aci:Denotazione_di_luogo\"><span style=\" text-decoration: underline; \">denotazione di luogo</span></a> specifica per gli <a href=\"/predicate/concept/aci:Stato_di_soggetto_registrato_per_veicolo\"><span style=\" text-decoration: underline; \">stati di soggetto registrati per veicolo</span></a>.</p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Ente_privato "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un <a href=\"/predicate/concept/aci:Ente_privato\"><span style=\" text-decoration: underline; \">ente privato</span></a>  è una <a href=\"/predicate/concept/aci:Persona_giuridica\"><span style=\" text-decoration: underline; \">persona giuridica</span></a>  creata secondo norme di diritto privato, destinata a perseguire interessi di carattere privato, ma che può anche svolgere, come gli <a href=\"/predicate/concept/aci:Ente_pubblico\"><span style=\" text-decoration: underline; \">enti pubblici</span></a> , funzioni amministrative.   </p></OntologyDescription>"^^xsd:string)
DisjointClasses(aci:Ente_privato aci:Ente_pubblico)
AnnotationAssertion(ms:classDescription aci:Ente_pubblico "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un <a href=\"/predicate/concept/aci:Ente_pubblico\"><span style=\" text-decoration: underline; \">ente pubblico</span></a>  è una <a href=\"/predicate/concept/aci:Persona_giuridica\"><span style=\" text-decoration: underline; \">persona giuridica</span></a>  creata secondo norme di diritto pubblico, attraverso cui la pubblica amministrazione svolge la sua funzione amministrativa. </p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Gli enti pubblici si contrappongono, quindi, agli <a href=\"/predicate/concept/aci:Ente_privato\"><span style=\" text-decoration: underline; \">enti privati</span></a> , persone giuridiche create secondo norme di diritto privato, che sono per lo più destinate a perseguire interessi di carattere privato, ma possono però anche svolgere funzioni amministrative.</p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">  </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Ente_territoriale "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un <a href=\"/predicate/concept/aci:Ente_territoriale\"><span style=\" text-decoration: underline; \">Ente territoriale</span></a>  è un ente pubblico locale , che ha tra i suoi elementi costitutivi il territorio, il quale è quindi essenziale per l'esistenza dell'ente. </p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Gli enti pubblici che non appartengono a questa categoria sono detti enti istituzionali.  E' ente territoriale la <a href=\"/predicate/concept/aci:Nazione\"><span style=\" text-decoration: underline; \">nazione</span></a> ma lo sono anche quegli enti locali per i quali il territorio della circoscrizione non è solo limite della competenza degli organi, ma anche elemento costitutivo dell'ente stesso, i cui organi di governo sono rappresentativi della popolazione residente. Con riferimento a questi enti locali si suole parlare di governo locale.  </p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Secondo l'art. 114 della Costituzione, la Repubblica Italiana è costituita da un ente territoriale nazionale, lo Stato, e dai seguenti enti territoriali:  </p>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">i <a href=\"/predicate/concept/aci:Comune\"><span style=\" text-decoration: underline; \">comuni</span></a>   </li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">le <a href=\"/predicate/concept/aci:Provincia\"><span style=\" text-decoration: underline; \">province</span></a>   </li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">le <a href=\"/predicate/concept/aci:Citta_metropolitana\"><span style=\" text-decoration: underline; \">città metropolitane</span></a> </li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"> le <a href=\"/predicate/concept/aci:Regione\"><span style=\" text-decoration: underline; \">regioni</span></a>   </li></ul>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Secondo il citato art. 114 &quot; essi sono enti autonomi con propri statuti, poteri e funzioni secondo i principi fissati dalla Costituzione&quot;.  Il territorio nazione è diviso in <a href=\"/predicate/concept/aci:Regione\"><span style=\" text-decoration: underline; \">regioni</span></a>    </p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Ente_territoriale ObjectSomeValuesFrom(aci:ha_circoscrizione owl:Thing))
SubClassOf(aci:Ente_territoriale aci:Ente_pubblico)
DisjointClasses(aci:Ente_territoriale aci:Luogo)
AnnotationAssertion(ms:classDescription aci:Entita_avente_stati "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Questo concetto rappresenta tutti gli oggetti che hanno sia proprietà che non cambiano nel tempo, o per le quali non interessa tenere traccia del cambiamento, e che quindi sono sempre costanti per tutta la durata di esistenza dell'oggetto stesso, sia proprietà che invece modificano il loro valore, e per le quali si vuole mantenere memoria storica del cambiamento, con il dettaglio dell'istante in cui il cambiamento è avvenuto. Ad esempio, i <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicoli</span></a> hanno <a href=\"/predicate/concept/aci:Stato\"><span style=\" text-decoration: underline; \">stati</span></a>. </p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">In particolare, in questa ontologia si è assunto che il <a href=\"/predicate/attribute/aci:numero_telaio\"><span style=\" text-decoration: underline; \">numero di telaio</span></a> sia una proprietà non mutevole di un veicolo, e quindi un veicolo ha sempre un numero di telaio associato che non cambia mai. Al contrario, la <a href=\"/predicate/concept/aci:Targa_veicolo\"><span style=\" text-decoration: underline; \">targa del veicolo</span></a> può cambiare durante la vita del veicolo stesso, ed è quindi associata allo <a href=\"/predicate/role/aci:ha_stato_di_veicolo\"><span style=\" text-decoration: underline; \">stato del veicolo</span></a>.</p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Entita_avente_stati ObjectSomeValuesFrom(aci:ha_stato owl:Thing))
DisjointClasses(aci:Entita_avente_stati aci:Entita_non_avente_stati)
AnnotationAssertion(ms:classDescription aci:Entita_del_mondo "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Entita_del_mondo\"><span style=\" text-decoration: underline; \">entità del mondo</span></a>  è un oggetto che corrisponde ad una parte della realtà che l'ontologia si impegna a riconoscere e classificare, prescindendo dalla conoscenza in merito alle proprietà di quell'oggetto (che può al limite essere inaffidabile o persino nulla).</p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Gli oggetti con queste caratteristiche che non sono classificabili come <a href=\"/predicate/concept/aci:Entita_del_mondo\"><span style=\" text-decoration: underline; \">entità del mondo</span></a>  sono le <a href=\"/predicate/concept/aci:Rappresentazione\"><span style=\" text-decoration: underline; \">rappresentazioni</span></a> , ovvero gli oggetti che contengono una descrizione di altri oggetti.   </p></OntologyDescription>"^^xsd:string)
DisjointClasses(aci:Entita_del_mondo aci:Rappresentazione)
AnnotationAssertion(ms:classDescription aci:Entita_non_avente_stati "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Questo concetto rappresenta tutti gli oggetti che hanno solo proprietà che non cambiano nel tempo, o per le quali non interessa tenere traccia del cambiamento. Questi oggetti hanno quindi proprietà che sono sempre costanti per tutta la durata di esistenza dell'oggetto stesso.</p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Estinzione_di_usufrutto "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un'<a href=\"/predicate/concept/aci:Estinzione_di_usufrutto\"><span style=\" text-decoration: underline; \">estinzione di usufrutto</span></a>  è una <a href=\"/predicate/concept/aci:Formalita\"><span style=\" text-decoration: underline; \">formalità</span></a>  (<a href=\"/predicate/concept/aci:Formalita_di_variazione\"><span style=\" text-decoration: underline; \">di variazione</span></a> ) avente per <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  l'<a href=\"/predicate/concept/aci:Finalita_estinzione_di_usufrutto\"><span style=\" text-decoration: underline; \">estinzione di un usufrutto</span></a>     </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Estinzione_di_usufrutto ObjectSomeValuesFrom(aci:richiesta_per_finalita aci:Finalita_estinzione_di_usufrutto))
AnnotationAssertion(ms:classDescription aci:Evento "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un <a href=\"/predicate/concept/aci:Evento\"><span style=\" text-decoration: underline; \">evento</span></a> è un accadimento che avviene in un certo istante temporale, ed è quindi visto come atomico rispetto allo scorrere del tempo (contrariamente a quanto avviene, ad esempio, per un oggetto avente stati). </p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un evento può concorrere a modificare lo <a href=\"/predicate/concept/aci:Stato\"><span style=\" text-decoration: underline; \">stato di un oggetto</span></a>. </p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Gli eventi sulla rappresentazione, ad esempio le trascrizioni operate dal PRA, concorrono sempre alla modifica di uno stato. La modifica dello stato di un oggetto si concretizza nella creazione di un nuovo stato corrente.</p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Evento DataSomeValuesFrom(aci:timestamp rdfs:Literal))
SubClassOf(aci:Evento aci:Entita_non_avente_stati)
AnnotationAssertion(ms:classDescription aci:Evento_con_impatto_su_periodo_tributario "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un <a href=\"/predicate/concept/aci:Evento_con_impatto_su_periodo_tributario\"><span style=\" text-decoration: underline; \">evento con impatto sul periodo tributario</span></a> è un particolare <a href=\"/predicate/concept/aci:Evento\"><span style=\" text-decoration: underline; \">evento</span></a> che comporta una modifica nel calcolo del dovuto sulla <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto\"><span style=\" text-decoration: underline; \">applicazione di bollo auto</span></a> per il periodo tributario successivo al verificarsi dell'evento.</p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Evento_con_impatto_su_periodo_tributario aci:Evento)
AnnotationAssertion(ms:classDescription aci:Femmina "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Femmina\"><span style=\" text-decoration: underline; \">femmina</span></a>  è una <a href=\"/predicate/concept/aci:Persona_fisica\"><span style=\" text-decoration: underline; \">persona fisica</span></a>  di sesso femminile.   </p></OntologyDescription>"^^xsd:string)
DisjointClasses(aci:Femmina aci:Maschio)
AnnotationAssertion(ms:classDescription aci:Filobus "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un <a href=\"/predicate/concept/aci:Filobus\"><span style=\" text-decoration: underline; \">filobus</span></a>  è un <a href=\"/predicate/concept/aci:Filoveicolo\"><span style=\" text-decoration: underline; \">filoveicolo</span></a>  strutturalmente destinato al trasporto esclusivo di <a href=\"/predicate/concept/aci:Passeggero\"><span style=\" text-decoration: underline; \">passeggeri</span></a> , anche con bagagli personali (più di 9 passeggeri, secondo il Codice della strada).</p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Filobus aci:Filoveicolo)
AnnotationAssertion(ms:classDescription aci:Filoveicolo "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un <a href=\"/predicate/concept/aci:Filoveicolo\"><span style=\" text-decoration: underline; \">filoveicolo</span></a>  è un <a href=\"/predicate/concept/aci:Veicolo_a_motore\"><span style=\" text-decoration: underline; \">veicolo a motore</span></a>  con le seguenti caratteristiche:</p>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">il motore è elettrico, ed è collegato, per la sua alimentazione. ad una linea aerea di contatto;</li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">non è vincolato da rotaie;</li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">è marciante su ruote gommate e non in sede propria;</li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">è munito di motore ausiliario a scoppio o diesel (non obbligatoriamente).  </li></ul>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">A loro volta si suddividono, come per gli autoveicoli, in filobus, filosnodati, filotreni, ecc... Prevalentemente circolano in aree urbane o suburbane e su percorsi prestabiliti.    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a> è lo scopo di una <a href=\"/predicate/concept/aci:Formalita\"><span style=\" text-decoration: underline; \">formalità</span></a>.</p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Finalita ObjectSomeValuesFrom(aci:ha_tipo_finalita owl:Thing))
EquivalentClasses(aci:Finalita ObjectSomeValuesFrom(ObjectInverseOf(aci:richiesta_per_finalita) owl:Thing))
AnnotationAssertion(ms:classDescription aci:Finalita_acquisto_di_possesso "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_acquisto_di_possesso\"><span style=\" text-decoration: underline; \">finalità di acquisto di possesso</span></a>  è una <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  che si consegue dopo la segnalazione di possesso (facoltativa) di un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  non registrato al PRA (ad esempio un veicolo del Corpo diplomatico).    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_annotazione_sospensione_carta_circolazione "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_annotazione_sospensione_carta_circolazione\"><span style=\" text-decoration: underline; \">finalità annotazione sospensione della carta di circolazione</span></a> è una <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a> che si consegue quando un la sospensione di una carta di circolazione viene trascritta nel PRA.</p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_annullamento_per_vendita_anteriore "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_annullamento_per_vendita_anteriore\"><span style=\" text-decoration: underline; \">finalità annullamento per vendita anteriore</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cancellazione_provvedimento_giudiziale\"><span style=\" text-decoration: underline; \">finalità cancellazione provvedimento giudiziale</span></a>  che si consegue quando un <a href=\"/predicate/concept/aci:Annullamento_per_vendita_anteriore\"><span style=\" text-decoration: underline; \">annullamento per vendita anteriore</span></a>  viene trascritto nel PRA.   </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_apposizione_visto_su_effetti "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_apposizione_visto_su_effetti\"><span style=\" text-decoration: underline; \">finalità apposizione visto su effetti</span></a>  è una <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  che si consegue con l'apposizione del visto (questa descrizione deve essere migliorata).   </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cancellazione_condiz_clausola "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  di <a href=\"/predicate/concept/aci:Finalita_cancellazione_condiz_clausola\"><span style=\" text-decoration: underline; \">cancellazione clausola</span></a>  è la finalità associata alla omonima <a href=\"/predicate/concept/aci:Formalita\"><span style=\" text-decoration: underline; \">formalità</span></a> , ovvero la <a href=\"/predicate/concept/aci:Cancellazione_condiz_clausola\"><span style=\" text-decoration: underline; \">cancellazione condizione di clausola</span></a> , che è una <a href=\"/predicate/concept/aci:Formalita_di_variazione\"><span style=\" text-decoration: underline; \">formalita di variazione</span></a> .    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cancellazione_congelamento_dei_beni "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cancellazione_congelamento_dei_beni\"><span style=\" text-decoration: underline; \">finalità cancellazione congelamento dei beni</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cancellazione_provvedimento_giudiziale\"><span style=\" text-decoration: underline; \">finalità cancellazione provvedimento giudiziale</span></a>  che si consegue quando la cancellazione di un <a href=\"/predicate/concept/aci:Autorizzazione_al_congelamento_dei_beni\"><span style=\" text-decoration: underline; \">congelamento di beni</span></a>  viene trascritta nel PRA.    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cancellazione_domanda_giudiziale "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cancellazione_domanda_giudiziale\"><span style=\" text-decoration: underline; \">finalità cancellazione domanda giudiziale</span></a>  è una <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  che si consegue quando la cancellazione di una  <a href=\"/predicate/concept/aci:Finalita_trascrizione_domanda_giudiziale\"><span style=\" text-decoration: underline; \">domanda giudiziale</span></a>  viene trascritta nel PRA.   </p></OntologyDescription>"^^xsd:string)
DisjointClasses(aci:Finalita_cancellazione_domanda_giudiziale aci:Finalita_trascrizione_domanda_giudiziale)
AnnotationAssertion(ms:classDescription aci:Finalita_cancellazione_ipoteca_o_privilegio "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cancellazione_ipoteca_o_privilegio\"><span style=\" text-decoration: underline; \">finalità di cancellazione di ipoteca o di privilegio</span></a>  è una <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalitа </span></a>  che si consegue quando la cancellazione di un'ipoteca o di un privilegio su un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  vengono iscritti nel PRA.    </p></OntologyDescription>"^^xsd:string)
DisjointClasses(aci:Finalita_cancellazione_ipoteca_o_privilegio aci:Finalita_riduzione_ipoteca_o_privilegio)
AnnotationAssertion(ms:classDescription aci:Finalita_cancellazione_locazione "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cancellazione_locazione\"><span style=\" text-decoration: underline; \">finalità cancellazione locazione</span></a>  è una <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  che si consegue quando la cancellazione di una locazione senza conducente viene iscritta nel PRA.    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cancellazione_patto_riservato_dominio_con_liberatoria "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Questa <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  ha come oggetto la cancellazione di un <a href=\"/predicate/concept/aci:Patto_RD_con_liberatoria\"><span style=\" text-decoration: underline; \">PRD con liberatoria</span></a> , ovvero un <a href=\"/predicate/concept/aci:Patto_RD\"><span style=\" text-decoration: underline; \">PRD</span></a>  in cui, a seguito del saldo dell'intero <a href=\"/predicate/attribute/aci:importo_a_concorrenza\"><span style=\" text-decoration: underline; \">importo a concorrenza</span></a> , è stato emesso l'atto di liberatoria.   </p></OntologyDescription>"^^xsd:string)
DisjointClasses(aci:Finalita_cancellazione_patto_riservato_dominio_con_liberatoria aci:Finalita_cancellazione_patto_riservato_dominio_con_risoluzione)
AnnotationAssertion(ms:classDescription aci:Finalita_cancellazione_patto_riservato_dominio_con_risoluzione "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Nella vendita con riservato dominio l'acquirente acquista la proprietà solamente con il pagamento dell'ultima rata del prezzo. Da questo momento l'acquirente può chiedere la cancellazione allo scopo di non far risultare il vincolo della riserva di proprietà.</p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cancellazione_per_sgravio_totale_per_indebito "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cancellazione_per_sgravio_totale_per_indebito\"><span style=\" text-decoration: underline; \">finalità cancellazione per sgravio totale per indebito</span></a> si riferisce ad provvedimento di fermo amministrativo che era stato indebitamente trascritto in quanto il credito vantato dal concessionario non era in realtà dovuto. La cancellazione viene effettuata su richiesta del concessionario.    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cancellazione_pignoramento "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Questa <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  è una particolare <a href=\"/predicate/concept/aci:Finalita_cancellazione_provvedimento_giudiziale\"><span style=\" text-decoration: underline; \">finalità di cancellazione di un provvedimento giudiziale</span></a> , che ha come obiettivo la cancellazione di un pignoramento.   </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cancellazione_provvedimento_giudiziale "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cancellazione_provvedimento_giudiziale\"><span style=\" text-decoration: underline; \">finalità cancellazione provvedimento giudiziale</span></a>  è una <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  che si consegue quando la cancellazione di un <a href=\"/predicate/concept/aci:Provvedimento_giudiziale\"><span style=\" text-decoration: underline; \">provvedimento giudiziale</span></a>  viene trascritta nel PRA.   </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cancellazione_sequestro "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Questa <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  è una particolare <a href=\"/predicate/concept/aci:Finalita_cancellazione_provvedimento_giudiziale\"><span style=\" text-decoration: underline; \">finalità di cancellazione di un provvedimento giudiziale</span></a> , che ha come obiettivo la cancellazione di un sequestro.    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione\"><span style=\" text-decoration: underline; \">finalità di cessazione di circolazione</span></a>  è una <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  che si consegue quando la cessazione della circolazione di un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  viene registrata nel PRA. Dal periodo impositivo successivo alla data del rilascio del Certificato di Radiazione, si interrompe l'obbligo del pagamento della tassa automobilistica (bollo auto).   </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_abbandono_in_depositorie_giudiziarie "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_abbandono_in_depositorie_giudiziarie\"><span style=\" text-decoration: underline; \">finalità cessazione della circolazione per abbandono in depositorie giudiziarie</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione\"><span style=\" text-decoration: underline; \">finalità di cessazione della circolazione</span></a>  che si consegue quando un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  sottoposto a sequestro e conservato in una depositoria giudiziaria è rimasto in tale sede oltre il tempo necessario ad accertare la possibile identificazione di un proprietario che provveda a farne reclamo ed è stato considerata \\cosa abbandonata ai sensi dell'articolo 923 del codice civile.   </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_annullamento_immatricolazione "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_annullamento_immatricolazione\"><span style=\" text-decoration: underline; \">finalità cessazione della circolazione per annullamento immatricolazione</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione\"><span style=\" text-decoration: underline; \">finalità di cessazione della circolazione</span></a>  che si consegue quando l'annullamento dell'immatricolazione di un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  viene registrato nel PRA.    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_articolo_159_codice_della_strada "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_articolo_159_codice_della_strada\"><span style=\" text-decoration: underline; \">finalità cessazione della circolazione per articolo 159 del Codice della Strada</span></a> si riferisce alla radiazione dal PRA dei veicoli in sosta che, per il loro stato o per altro fondato motivo si possa ritenere che siano stati abbandonati.</p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_articolo_215_codice_della_strada "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_articolo_215_codice_della_strada\"><span style=\" text-decoration: underline; \">finalità cessazione della circolazione per articolo 215 del Codice della Strada</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_abbandono_in_depositorie_giudiziarie\"><span style=\" text-decoration: underline; \">finalità cessazione della circolazione per abbandono in depositorie giudiziarie</span></a>  che si consegue quando un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  rimosso per violazione delle norme del codice della strada non è stato reclamato dal proprietario o intestatario del documento di circolazione entro i termini previsti dall'articolo 215 del Codice della Strada ed è quindi stato sottoposto ad alienazione o demolizione.    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_articolo_29_5B_Legge_30_97 "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_articolo_29_5B_Legge_30_97\"><span style=\" text-decoration: underline; \">finalità cessazione della circolazione per articolo 29 comma 5bis della legge 30 del 1997</span></a> si riferisce alle formalità di radiazione che, in presenza di determinati requisiti, erano esenti dal pagamento dell'imposta di bollo e degli emolumenti.  </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_cambio_di_categoria "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_cambio_di_categoria\"><span style=\" text-decoration: underline; \">finalità cessazione della circolazione per cambio di categoria</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione\"><span style=\" text-decoration: underline; \">finalità di cessazione della circolazione</span></a>  che si consegue quando il cambio di destinazione d'uso di un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  viene registrato nel PRA e quindi, nella categoria di provenienza, il veicolo deve considerarsi non più esistente e quindi cessato. La categoria di appartenenza di un veicolo è determinata dal costruttore al momento della richiesta di omologazione del tipo o di accertamento dei requisiti di idoneità alla circolazione. Lo stesso costruttore ne attesta la rispondenza alle direttive comunitarie particolari ovvero, laddove previsto, alle norme nazionali. L'appartenenza di un veicolo ad una determinata categoria consegue, quindi, alla progettazione e alla costruzione dello stesso in conformità alle direttive particolari, aventi ambiti di applicazione differenziati a seconda della categoria d'appartenenza dei veicoli. Com'è noto, l'elenco delle direttive particolari applicabili alle diverse categorie dei veicoli è riportato nella direttiva 70&quot;&gt;156&quot;&gt;CEE e successive modifiche ed integrazioni (92&quot;&gt;53&quot;&gt;CEE, 98&quot;&gt;14&quot;&gt;CEE e 2001&quot;&gt;116&quot;&gt;CE). Ne consegue, in linea di principio, che l'inquadramento in una categoria diversa da quella originaria può ritenersi ammissibile alla condizione che il veicolo modificato risulti conforme a tutte le direttive particolari e alle norme nazionali vigenti per la categoria nella quale è richiesto l'inquadramento. Tale conformità, per quanto sopra riportato, deve essere attestata dal costruttore del veicolo in maniera diretta, con apposito nulla osta alla modifica, ovvero con l'attestazione che il veicolo è reso conforme ad un tipo di veicolo già omologato.    </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_cancellazione_articolo_94_bis_CdS "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_cancellazione_articolo_94_bis_CdS\"><span style=\" text-decoration: underline; \">finalità di cessazione di circolazione per cancellazione articolo 94 bis del Codice della Strada</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione\"><span style=\" text-decoration: underline; \">finalità di cessazione della circolazione</span></a>  che si consegue un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  è riconosciuto come intestato in modo simulato.   </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_cancellazione_ex_art_80 "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_cancellazione_ex_articolo_80\"><span style=\" text-decoration: underline; \">finalità cessazione della circolazione per cancellazione ex art. 80</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione\"><span style=\" text-decoration: underline; \">finalità di cessazione della circolazione</span></a> che si consegue quando la certificazione della revisione di un veicolo è accertata falsa dagli organi competenti, come previsto dalla art. 80 comma 16 del CDS.</p></OntologyDescription>"^^xsd:string)
DisjointClasses(aci:Finalita_cessazione_circolazione_per_cancellazione_ex_art_80 aci:Finalita_cessazione_circolazione_per_ritiro_in_aree_private)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_conservazione_musei "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_conservazione_musei\"><span style=\" text-decoration: underline; \">finalità cessazione della circolazione per conservazione nei musei</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione\"><span style=\" text-decoration: underline; \">finalità di cessazione della circolazione</span></a>  che si consegue quando la riclassificazione di un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  come veicolo d'epoca, di interesse storico e collezionistico, viene registrata nel PRA. I veicoli d'epoca sono motoveicoli e autoveicoli cancellati dal PRA, non più in circolazione ma destinati alla loro conservazione in musei o locali pubblici e privati, ai fini della salvaguardia delle originarie caratteristiche tecniche specifiche della casa costruttrice, o comunque non adeguati nei requisiti, nei dispositivi e negli equipaggiamenti alle vigenti prescrizioni stabilite per l'ammissione alla circolazione. Tali veicoli sono iscritti in apposito elenco presso il Centro storico del Dipartimento per i trasporti terrestri.   </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_contributo_governativo "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_contributo_governativo\"><span style=\" text-decoration: underline; \">finalità di cessazione circolazione per contributo governativo</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione\"><span style=\" text-decoration: underline; \">finalità di cessazione della circolazione</span></a>  che si consegue quando la cessazione della circolazione di un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  per contributo governativo (rottamazione) viene registrata nel PRA.   </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_decreto_ministeriale_460_22_10_99 "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_decreto_ministeriale_460_22_10_99\"><span style=\" text-decoration: underline; \">finalità cessazione della circolazione per decreto ministeriale 460 del 10&quot;&gt;11&quot;&gt;1999</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_abbandono_in_depositorie_giudiziarie\"><span style=\" text-decoration: underline; \">finalità cessazione della circolazione per abbandono in depositorie giudiziarie</span></a>  che si consegue quando un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  rinvenuto su un'area pubblica in condizioni  da far presumere  lo stato  di  abbandono, dopo la rimozione e conservazione in una depositoria giudiziaria per il tempo necessario ad accertare la possibile identificazione di un proprietario che provveda a farne reclamo, è stato considerato cosa abbandonata ai sensi dell'articolo 923 del codice civile.   </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_demolizione "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_demolizione\"><span style=\" text-decoration: underline; \">finalità di cessazione di circolazione per demolizione</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione\"><span style=\" text-decoration: underline; \">finalità di cessazione di circolazione</span></a>  che si consegue quando la cessazione della circolazione di un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  per demolizione viene registrata nel PRA.   </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_distruzione_o_incendio "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_distruzione_o_incendio\"><span style=\" text-decoration: underline; \">finalità di cessazione di circolazione per distruzione o incendio</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione\"><span style=\" text-decoration: underline; \">finalità di cessazione della circolazione</span></a>  che si consegue quando la cessazione della circolazione di un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  per distruzione o incendio viene registrata nel PRA.   </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_esportazione "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_esportazione\"><span style=\" text-decoration: underline; \">finalita cessazione circolazione per esportazione</span></a>  è una particolare <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione\"><span style=\" text-decoration: underline; \">finalità di cessazione della circolazione</span></a>  causata dalla esportazione del <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  al di fuori dei confini nazionali.    </p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Finalita_cessazione_circolazione_per_esportazione ObjectUnionOf(aci:Finalita_cessazione_circolazione_per_esportazione_UE aci:Finalita_cessazione_circolazione_per_esportazione_fuori_UE))
AnnotationAssertion(ms:classDescription aci:Finalita_cessazione_circolazione_per_esportazione_UE "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione_per_esportazione_UE\"><span style=\" text-decoration: underline; \">finalità di cessazione di circolazione per esportazione</span></a>  è una <a href=\"/predicate/concept/aci:Finalita_cessazione_circolazione\"><span style=\" text-decoration: underline; \">finalità di cessazione di circolazione</span></a>  che si consegue quando la cessazione della circolazione di un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a>  per esportazione in un altro paese della Unione europea viene registrata nel PRA.    </p></OntologyDescription>"^^xsd:string)
