AnnotationAssertion(ms:classDescription aci:Rappresentazione_nel_ruolo_tributario "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Denota le rappresentazioni (di <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicoli</span></a> e loro <a href=\"/predicate/concept/aci:Stato_di_veicolo\"><span style=\" text-decoration: underline; \">stati</span></a>) fatte nell'ambito del ruolo tributario, cioè nei sistemi relativi alla gestione delle tasse automobilistiche.</p></OntologyDescription>"^^xsd:string)
