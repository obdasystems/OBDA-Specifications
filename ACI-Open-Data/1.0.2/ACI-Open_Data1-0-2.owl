AnnotationAssertion(ms:classDescription aci:Patto_RD_risolto "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un <a href=\"/predicate/concept/aci:Patto_RD\"><span style=\" text-decoration: underline; \">patto di riservato dominio</span></a>  è <a href=\"/predicate/concept/aci:Patto_RD_risolto\"><span style=\" text-decoration: underline; \">risolto</span></a> quando il soggetto acquirente è inadempiente, come dall'art. 1526 cod. civ, avendo superato il mancato pagamento dell'ottava parte del prezzo del bene.</p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Patto_RD_risolto DataSomeValuesFrom(aci:data_risoluzione rdfs:Literal))
AnnotationAssertion(ms:classDescription aci:Perdita_di_possesso "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Perdita_di_possesso\"><span style=\" text-decoration: underline; \">perdita di possesso</span></a>  intesa come <a href=\"/predicate/concept/aci:Formalita\"><span style=\" text-decoration: underline; \">formalità</span></a>  (<a href=\"/predicate/concept/aci:Formalita_di_variazione\"><span style=\" text-decoration: underline; \">di variazione</span></a> ) ha per <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a> , appunto, una <a href=\"/predicate/concept/aci:Finalita_perdita_di_possesso\"><span style=\" text-decoration: underline; \">perdita di possesso</span></a>     </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Perdita_di_possesso ObjectSomeValuesFrom(aci:richiesta_per_finalita aci:Finalita_perdita_di_possesso))
AnnotationAssertion(ms:classDescription aci:Periodo_fiscale_per_tributo_operativo "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un <a href=\"/predicate/concept/aci:Periodo_fiscale_per_tributo_operativo\"><span style=\" text-decoration: underline; \">periodo fiscale per tributo operativo</span></a>  è un periodo associato ad un certo <a href=\"/predicate/concept/aci:Tributo_operativo\"><span style=\" text-decoration: underline; \">t</span></a><a href=\"/predicate/concept/aci:Tributo_operativo_con_periodi_fiscali\"><span style=\" text-decoration: underline; \">ributo operativo con periodi fiscali</span></a>, durante il quale le condizioni che definiscono l'<a href=\"/predicate/concept/aci:Applicazione_di_tributo\"><span style=\" text-decoration: underline; \">applicazione</span></a> di quel tributo non vengono cambiate. In particolare, le <a href=\"/predicate/concept/aci:Classe_contributiva\"><span style=\" text-decoration: underline; \">classi contributive</span></a> (ad esempio <a href=\"/predicate/concept/aci:Classe_contributiva_di_veicolo\"><span style=\" text-decoration: underline; \">relative ai veicoli</span></a> in relazione ai <a href=\"/predicate/concept/aci:Periodo_fiscale_per_tributo_operativo\"><span style=\" text-decoration: underline; \">periodi fiscali</span></a> del <a href=\"/predicate/concept/aci:Bollo_auto\"><span style=\" text-decoration: underline; \">Bollo Auto</span></a> di una certa <a href=\"/predicate/concept/aci:Regione\"><span style=\" text-decoration: underline; \">regione</span></a>) e le regole di definizione della <a href=\"/predicate/concept/aci:Classe_contributiva\"><span style=\" text-decoration: underline; \">classe</span></a> da utilizzare relativamente ad una <a href=\"/predicate/concept/aci:Applicazione_di_tributo\"><span style=\" text-decoration: underline; \">applicazione del tributo</span></a> (ad esempio per l'<a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto\"><span style=\" text-decoration: underline; \">applicazione del bollo auto</span></a> ad un determinato veicolo) restano fisse per tutta la dudata del <a href=\"/predicate/concept/aci:Periodo_fiscale_per_tributo_operativo\"><span style=\" text-decoration: underline; \">periodo fiscale.</span></a> </p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Si noti che le <a href=\"/predicate/concept/aci:Classe_contributiva\"><span style=\" text-decoration: underline; \">classi contributive</span></a>  sono sempre associate ad un solo <a href=\"/predicate/concept/aci:Periodo_fiscale_per_tributo_operativo\"><span style=\" text-decoration: underline; \">periodo fiscale</span></a>: il cambiamento di periodo fiscale comporta il cambiamento di tutte le classi contributive del <a href=\"/predicate/concept/aci:Tributo_operativo_con_periodi_fiscali\"><span style=\" text-decoration: underline; \">tributo</span></a>.    </p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Periodo_fiscale_per_tributo_operativo ObjectSomeValuesFrom(ObjectInverseOf(aci:ha_valenza_in) owl:Thing))
EquivalentClasses(aci:Periodo_fiscale_per_tributo_operativo DataSomeValuesFrom(aci:fine_periodo_fiscale rdfs:Literal))
EquivalentClasses(aci:Periodo_fiscale_per_tributo_operativo DataSomeValuesFrom(aci:inizio_periodo_fiscale rdfs:Literal))
SubClassOf(aci:Periodo_fiscale_per_tributo_operativo aci:Entita_non_avente_stati)
