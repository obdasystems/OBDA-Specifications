EquivalentClasses(aci:Agevolazione DataSomeValuesFrom(aci:tipo_di_agevolazione rdfs:Literal))
AnnotationAssertion(ms:classDescription aci:Alimentazione "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una istanza di questo concetto denota una <a href=\"/predicate/concept/aci:Alimentazione\"><span style=\" text-decoration: underline; \">alimentazione</span></a>, cioè la fonte di energia, di un v<a href=\"/predicate/concept/aci:Veicolo_a_motore\"><span style=\" text-decoration: underline; \">eicolo a motore</span></a>. Il nome del concetto deriva da &quot;impianto di alimentazione&quot;, organo dei veicoli a motore. Questo concetto in particolare denota le alimentazioni così così come usualmente classificate nei sistemi informativi di ACI.</p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Annotazione_locazione "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Annotazione_locazione\"><span style=\" text-decoration: underline; \">annotazione locazione</span></a>  è una <a href=\"/predicate/concept/aci:Formalita\"><span style=\" text-decoration: underline; \">formalità</span></a>  (<a href=\"/predicate/concept/aci:Formalita_di_variazione\"><span style=\" text-decoration: underline; \">di variazione</span></a> ) avente per <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  una delle seguenti:</p>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><a href=\"/predicate/concept/aci:Finalita_iscrizione_locazione\"><span style=\" text-decoration: underline; \">l'iscrizione di una locazione</span></a> </li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><a href=\"/predicate/concept/aci:Finalita_cessione_locazione\"><span style=\" text-decoration: underline; \">la cessione di una locazione</span></a> </li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><a href=\"/predicate/concept/aci:Finalita_proroga_locazione\"><span style=\" text-decoration: underline; \">la proroga di una locazione</span></a> </li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><a href=\"/predicate/concept/aci:Finalita_cancellazione_locazione\"><span style=\" text-decoration: underline; \">la cancellazione di una locazione</span></a>    </li></ul></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Annotazione_locazione ObjectSomeValuesFrom(aci:richiesta_per_finalita aci:Finalita_gestione_locazione))
AnnotationAssertion(ms:classDescription aci:Annotazione_sospensione_carta_circolazione "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Annotazione_sospensione_carta_circolazione\"><span style=\" text-decoration: underline; \">annotazione sospensione carta di circolazione</span></a>  è una <a href=\"/predicate/concept/aci:Formalita\"><span style=\" text-decoration: underline; \">formalità</span></a>  (<a href=\"/predicate/concept/aci:Formalita_di_variazione\"><span style=\" text-decoration: underline; \">di variazione</span></a> ) avente per <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  l'<a href=\"/predicate/concept/aci:Finalita_annotazione_sospensione_carta_circolazione\"><span style=\" text-decoration: underline; \">l'annotazione della sospensione di una carta di circolazione</span></a>    </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Annotazione_sospensione_carta_circolazione ObjectSomeValuesFrom(aci:richiesta_per_finalita aci:Finalita_annotazione_sospensione_carta_circolazione))
AnnotationAssertion(ms:classDescription aci:Annullamento_per_vendita_anteriore "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Un <a href=\"/predicate/concept/aci:Annullamento_per_vendita_anteriore\"><span style=\" text-decoration: underline; \">annullamento per vendita anteriore</span></a>  è un <a href=\"/predicate/concept/aci:Provvedimento\"><span style=\" text-decoration: underline; \">provvedimento</span></a>  (<a href=\"/predicate/concept/aci:Provvedimento_di_cancellazione\"><span style=\" text-decoration: underline; \">di cancellazione</span></a> ) annulla l'effetto di un <a href=\"/predicate/concept/aci:Provvedimento_di_fermo_amministrativo\"><span style=\" text-decoration: underline; \">provvedimento di fermo amministrativo</span></a>  nella circostanza di vendita del bene anteriore al fermo.   </p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Applicazione_di_IPT "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Applicazione_di_IPT\"><span style=\" text-decoration: underline; \">applicazione di IPT</span></a>  è una <a href=\"/predicate/concept/aci:Applicazione_di_tributo_a_veicolo\"><span style=\" text-decoration: underline; \">applicazione di tributo a veicolo</span></a>  che applica, come <a href=\"/predicate/concept/aci:Classe_contributiva\"><span style=\" text-decoration: underline; \">classe contributiva</span></a> , una <a href=\"/predicate/concept/aci:Classe_contributiva_IPT\"><span style=\" text-decoration: underline; \">classe contributiva IPT</span></a> .   </p></OntologyDescription>"^^xsd:string)
