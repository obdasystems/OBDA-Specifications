AnnotationAssertion(ms:classDescription aci:Classe_contributiva_bollo_auto "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Classe_contributiva_bollo_auto\"><span style=\" text-decoration: underline; \">classe contributiva bollo auto</span></a>  è una <a href=\"/predicate/concept/aci:Classe_contributiva\"><span style=\" text-decoration: underline; \">classe contributiva</span></a>  che si riferisce, come tributo, al <a href=\"/predicate/concept/aci:Bollo_auto_di_regione\"><span style=\" text-decoration: underline; \">bollo auto</span></a>  di una certa <a href=\"/predicate/concept/aci:Regione\"><span style=\" text-decoration: underline; \">regione</span></a> .   </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Classe_contributiva_bollo_auto ObjectAllValuesFrom(ObjectInverseOf(aci:ha_classe_contributiva) ObjectSomeValuesFrom(ObjectInverseOf(aci:ha_valenza_in) aci:Bollo_auto)))
SubClassOf(aci:Classe_contributiva_bollo_auto ObjectAllValuesFrom(ObjectInverseOf(aci:per_classe_contributiva_di_veicolo) aci:Applicazione_di_bollo_auto))
AnnotationAssertion(ms:classDescription aci:Classe_contributiva_di_veicolo "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Classe_contributiva\"><span style=\" text-decoration: underline; \">classe contributiva</span></a>  è <a href=\"/predicate/concept/aci:Classe_contributiva_di_veicolo\"><span style=\" text-decoration: underline; \">di veicolo</span></a>  se associa la specifica di un certo insieme di <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicoli</span></a>  ad una funzione che lega ogni veicolo dell'insieme all'importo di un <a href=\"/predicate/concept/aci:Tributo\"><span style=\" text-decoration: underline; \">tributo</span></a>  (o più precisamente l'<a href=\"/predicate/concept/aci:Applicazione_di_tributo_a_veicolo\"><span style=\" text-decoration: underline; \">applicazione</span></a>  del tributo a quel veicolo). Si noti che ogni classe contributiva di veicolo è definita:</p>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">in relazione ad un tributo <a href=\"/predicate/concept/aci:Tributo_operativo\"><span style=\" text-decoration: underline; \">operativo</span></a> </li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">per un certo <a href=\"/predicate/concept/aci:Periodo_fiscale_per_tributo_operativo\"><span style=\" text-decoration: underline; \">periodo fiscale</span></a> (solo se il tributo operativo è <a href=\"/predicate/concept/aci:Tributo_operativo_con_periodi_fiscali\"><span style=\" text-decoration: underline; \">con periodi fiscali</span></a> ), nel senso che ogni tributo ha classi contributive sue proprie e che, dato un tributo questo, se è con periodi fiscali, ha classi contributive diverse in diversi periodi fiscali.</li></ul>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Classe_contributiva_di_veicolo aci:Classe_contributiva)
DisjointClasses(aci:Classe_contributiva_di_veicolo aci:Classe_contributiva_imposta_di_bollo)
AnnotationAssertion(ms:classDescription aci:Classe_contributiva_imposta_di_bollo "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Classe_contributiva_imposta_di_bollo\"><span style=\" text-decoration: underline; \">classe contributiva per imposta di bollo</span></a>  è una <a href=\"/predicate/concept/aci:Classe_contributiva\"><span style=\" text-decoration: underline; \">classe contributiva</span></a> relativa al tributo <a href=\"/predicate/concept/aci:Applicazione_imposta_di_bollo\"><span style=\" text-decoration: underline; \">imposta di bollo</span></a>. Le sue caratteristiche variano principalmente in base al tipo di atto a cui si applica.</p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Classe_contributiva_imposta_di_bollo ObjectSomeValuesFrom(aci:classe_per_tipo_di_atto owl:Thing))
