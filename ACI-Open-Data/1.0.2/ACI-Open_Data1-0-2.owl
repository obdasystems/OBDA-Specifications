AnnotationAssertion(ms:classDescription aci:Applicazione_di_bollo_auto_non_esigibile "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto_non_esigibile\"><span style=\" text-decoration: underline; \">applicazione di bollo auto non esigibile</span></a>  è una particolare tipologia di <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto\"><span style=\" text-decoration: underline; \">applicazione di bollo auto</span></a> non esigibile a causa di una <a href=\"/predicate/concept/aci:Sospensione\"><span style=\" text-decoration: underline; \">sospensione</span></a> .</p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Applicazione_di_bollo_auto_per_integrazione_per_modifica_dato_tecnico "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto_per_integrazione_per_modifica_dato_tecnico\"><span style=\" text-decoration: underline; \">applicazione di bollo auto per integrazione per modifica di dato tecnico</span></a>  è una particolare tipologia di <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto\"><span style=\" text-decoration: underline; \">applicazione di bollo auto</span></a> che si applica quando è stato effettuato, nel corso dell'ultimo periodo tributario, una modifica ai dati tecnici del <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a> .</p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Applicazione_di_bollo_auto_per_primo_pagamento "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto_per_primo_pagamento\"><span style=\" text-decoration: underline; \">Applicazione di bollo auto per primo pagamento</span></a> è una particolare tipologia di <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto\"><span style=\" text-decoration: underline; \">applicazione di bollo auto</span></a> avente come causale di pagamento il primo pagamento. </p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Ogni <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto\"><span style=\" text-decoration: underline; \">applicazione di bollo auto</span></a> successivo al primo, per quel particolare veicolo, fatta eccezione per casi particolari quali <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto_per_integrazione_per_modifica_dato_tecnico\"><span style=\" text-decoration: underline; \">integrazioni per modifica dei dati tecnici del veicolo</span></a> sarà una <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto_per_rinnovo\"><span style=\" text-decoration: underline; \">applicazione con causale di pagamento il rinnovo del bollo auto</span></a> .</p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Applicazione_di_bollo_auto_per_rientro "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto_per_rientro\"><span style=\" text-decoration: underline; \">applicazione di bollo auto per rientro</span></a>  è una particolare tipologia di <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto\"><span style=\" text-decoration: underline; \">applicazione di bollo auto</span></a> che si applica quando è stato effettuato, nel corso dell'ultimo periodo tributario, un rientro da una esenzione o da una regione non convenzionata.</p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Applicazione_di_bollo_auto_per_rinnovo "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto_per_rinnovo\"><span style=\" text-decoration: underline; \">applicazione di bollo auto per rinnovo</span></a>  è una particolare tipologia di <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto\"><span style=\" text-decoration: underline; \">applicazione di bollo auto</span></a> avente come causale di pagamento il rinnovo del pagamento del bollo auto.</p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Applicazione_di_bollo_auto_per_veicolo_storico "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto_per_veicolo_storico\"><span style=\" text-decoration: underline; \">applicazione di bollo auto per veicolo storico</span></a>  è una particolare tipologia di <a href=\"/predicate/concept/aci:Applicazione_di_bollo_auto\"><span style=\" text-decoration: underline; \">applicazione di bollo auto</span></a> che si applica quando il <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a> è classificato come <a href=\"/predicate/concept/aci:Stato_di_veicolo_di_epoca\"><span style=\" text-decoration: underline; \">d'epoca</span></a> .</p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Applicazione_di_tributo "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una istanza di questo concetto rappresenta una <a href=\"/predicate/concept/aci:Applicazione_di_tributo_a_veicolo\"><span style=\" text-decoration: underline; \">applicazione di un tributo</span></a>, e cioè l'azione di determinare l<a href=\"/predicate/attribute/aci:importo_dovuto\"><span style=\" text-decoration: underline; \">'importo dovuto</span></a> relativamente ad un certo <a href=\"/predicate/concept/aci:Tributo_operativo\"><span style=\" text-decoration: underline; \">tributo (operativo)</span></a> da parte di un <a href=\"/predicate/concept/aci:Soggetto\"><span style=\" text-decoration: underline; \">soggetto</span></a>.</p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Applicazione_di_tributo ObjectSomeValuesFrom(aci:relativa_a_tributo_operativo owl:Thing))
