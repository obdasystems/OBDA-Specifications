AnnotationAssertion(ms:classDescription aci:Applicazione_di_IPT_con_agevolazione_per_stato_di_calamita "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Questo concetto denota <a href=\"/predicate/concept/aci:Applicazione_di_IPT\"><span style=\" text-decoration: underline; \">applicazioni di IPT</span></a>  <a href=\"/predicate/concept/aci:Agevolazione\"><span style=\" text-decoration: underline; \">con agevolazione</span></a> consentite a seguito del riconoscimento di una situazione di calamità naturale per un'area geografica (eventi sismici, alluvione ecc.)</p></OntologyDescription>"^^xsd:string)
AnnotationAssertion(ms:classDescription aci:Applicazione_di_IPT_con_agevolazione_per_trasferimento_proprieta_a_concessionario "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Questo concetto denota <a href=\"/predicate/concept/aci:Applicazione_di_IPT\"><span style=\" text-decoration: underline; \">applicazioni di IPT</span></a>  <a href=\"/predicate/concept/aci:Agevolazione\"><span style=\" text-decoration: underline; \">con agevolazione</span></a> consentite nel caso in cui un veicolo venga venduto ad un concessionario.</p></OntologyDescription>"^^xsd:string)
