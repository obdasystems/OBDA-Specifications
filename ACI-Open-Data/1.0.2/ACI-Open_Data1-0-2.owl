EquivalentClasses(aci:Possesso_di_funzionalita ObjectSomeValuesFrom(aci:possesso_relativo_a owl:Thing))
EquivalentClasses(aci:Possesso_di_funzionalita ObjectSomeValuesFrom(ObjectInverseOf(aci:est_possessore_di) owl:Thing))
EquivalentClasses(aci:Possesso_di_funzionalita DataSomeValuesFrom(aci:data_inizio_possesso rdfs:Literal))
EquivalentClasses(aci:Possesso_di_funzionalita DataSomeValuesFrom(aci:tipo_di_possesso_di_funzionalita rdfs:Literal))
SubClassOf(aci:Possesso_di_funzionalita aci:Entita_non_avente_stati)
AnnotationAssertion(ms:classDescription aci:Postergazione_ipoteca "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Postergazione_ipoteca\"><span style=\" text-decoration: underline; \">postergazione ipoteca</span></a>  è una <a href=\"/predicate/concept/aci:Formalita\"><span style=\" text-decoration: underline; \">formalità</span></a>  (<a href=\"/predicate/concept/aci:Formalita_di_variazione\"><span style=\" text-decoration: underline; \">di variazione</span></a> ) avente per <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a>  la <a href=\"/predicate/concept/aci:Finalita_postergazione_ipoteca\"><span style=\" text-decoration: underline; \">postergazione di un'ipoteca</span></a>     </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Postergazione_ipoteca ObjectSomeValuesFrom(aci:richiesta_per_finalita aci:Finalita_postergazione_ipoteca))
AnnotationAssertion(ms:classDescription aci:Prima_iscrizione_di_veicolo_nuovo "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Prima_iscrizione_di_veicolo_nuovo\"><span style=\" text-decoration: underline; \">prima iscrizione di veicolo nuovo</span></a>  è una <a href=\"/predicate/concept/aci:Formalita\"><span style=\" text-decoration: underline; \">formalità</span></a> avente per <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a> la <a href=\"/predicate/concept/aci:Finalita_prima_iscrizione_di_veicolo_nuovo\"><span style=\" text-decoration: underline; \">prima iscrizione di un veicolo nuovo</span></a>     </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Prima_iscrizione_di_veicolo_nuovo ObjectSomeValuesFrom(aci:richiesta_per_finalita aci:Finalita_prima_iscrizione_di_veicolo_nuovo))
AnnotationAssertion(ms:classDescription aci:Prima_iscrizione_di_veicolo_usato "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Prima_iscrizione_di_veicolo_usato\"><span style=\" text-decoration: underline; \">prima iscrizione di veicolo usato</span></a>  è una <a href=\"/predicate/concept/aci:Formalita\"><span style=\" text-decoration: underline; \">formalità</span></a> avente per <a href=\"/predicate/concept/aci:Finalita\"><span style=\" text-decoration: underline; \">finalità</span></a> la <a href=\"/predicate/concept/aci:Finalita_prima_iscrizione_di_veicolo_usato\"><span style=\" text-decoration: underline; \">prima iscrizione di un veicolo usato</span></a>     </p></OntologyDescription>"^^xsd:string)
SubClassOf(aci:Prima_iscrizione_di_veicolo_usato ObjectSomeValuesFrom(aci:richiesta_per_finalita aci:Finalita_prima_iscrizione_di_veicolo_usato))
AnnotationAssertion(ms:classDescription aci:Proprieta "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Proprieta\"><span style=\" text-decoration: underline; \">proprietà </span></a>  è un legame tra un <a href=\"/predicate/concept/aci:Veicolo\"><span style=\" text-decoration: underline; \">veicolo</span></a> , in un certo suo <a href=\"/predicate/concept/aci:Stato_di_veicolo\"><span style=\" text-decoration: underline; \">stato</span></a> , e un <a href=\"/predicate/concept/aci:Soggetto\"><span style=\" text-decoration: underline; \">soggetto</span></a>  che, in quello stato, ne è <a href=\"/predicate/role/aci:est_proprietario_di\"><span style=\" text-decoration: underline; \">proprietario</span></a>  per una certa <a href=\"/predicate/attribute/aci:percentuale_proprieta\"><span style=\" text-decoration: underline; \">percentuale</span></a>  . </p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Si noti che il concetto di proprietà si riferisce al veicolo come oggetto, stabilendo il diritto del proprietario di disporre di esso come valore economico. In linea generale ciò significa che il proprietario (in ragione della percentuale di proprietà ) ha a priori (ovvero a meno di fatti limitativi) i seguenti diritti: </p>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">il diritto di cedere la proprietà del veicolo ottenendo un corrispondente beneficio economico</li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">il diritto di <a href=\"/predicate/concept/aci:Possesso_di_funzionalita\"><span style=\" text-decoration: underline; \">possesso</span></a>  del veicolo</li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">il diritto di cedere tale <a href=\"/predicate/concept/aci:Possesso_di_funzionalita\"><span style=\" text-decoration: underline; \">possesso</span></a>  ottenendo un beneficio economico   </li></ul>
<p style=\"-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\"><br /></p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">La cessione del possesso costituisce un fatto che limita il diritto di possesso e che può limitare il diritto di cedere la proprietà.    </p></OntologyDescription>"^^xsd:string)
EquivalentClasses(aci:Proprieta ObjectSomeValuesFrom(aci:proprieta_di owl:Thing))
EquivalentClasses(aci:Proprieta ObjectSomeValuesFrom(aci:proprieta_relativa_a owl:Thing))
EquivalentClasses(aci:Proprieta DataSomeValuesFrom(aci:data_inizio_proprieta rdfs:Literal))
EquivalentClasses(aci:Proprieta DataSomeValuesFrom(aci:percentuale_proprieta rdfs:Literal))
SubClassOf(aci:Proprieta aci:Entita_non_avente_stati)
AnnotationAssertion(ms:classDescription aci:Proprieta_soggetta_a_trust "<OntologyDescription style=\"   font-weight:400; font-style:normal;\">
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Una <a href=\"/predicate/concept/aci:Proprieta\"><span style=\" text-decoration: underline; \">proprieta</span></a>  di un <a href=\"/predicate/concept/aci:Stato_di_veicolo\"><span style=\" text-decoration: underline; \">veicolo in un suo stato</span></a>  è <a href=\"/predicate/concept/aci:Proprieta_soggetta_a_trust\"><span style=\" text-decoration: underline; \">soggetta a trust</span></a>  quando un <a href=\"/predicate/concept/aci:Soggetto\"><span style=\" text-decoration: underline; \">soggetto</span></a>  che dispone il trust (<a href=\"/predicate/role/aci:est_settler_di\"><span style=\" text-decoration: underline; \">settler</span></a>  ) trasferisce i diritti e poteri del <a href=\"/predicate/concept/aci:Proprieta\"><span style=\" text-decoration: underline; \">proprietario</span></a>  ad un altro soggetto (il <a href=\"/predicate/role/aci:est_trustee_di\"><span style=\" text-decoration: underline; \">trustee</span></a> ) per uno scopo prestabilito (in generale per la cura degli interessi del proprietario stesso). </p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">L'effetto principale dell'istituzione di un trust è la segregazione patrimoniale, in virtù della quale i beni conferiti nel trust costituiscono un patrimonio separato rispetto al patrimonio del trustee, con l'effetto che non possono essere escussi dai creditori del trustee, del disponente o del beneficiario. </p>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Il trustee acquista, in seguito all'atto di dotazione del proprietario, la titolarità del bene solo in tale sua qualità : ciò comporta che egli subisce i limiti imposti dal settler per il perseguimento dello scopo del trust, limiti che si manifestano nella destinazione dell'utilizzo del bene, dei proventi e dei frutti (o dei redditi comunque attinenti direttamente al bene) a favore dei soggetti beneficiari. L'art. 2 della Convenzione dell'Aja del 1985 prevede come elementi essenziali del trust:</p>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">i beni vincolati nel trust sono una massa distinta e non sono parte del patrimonio del trustee;</li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">i beni vincolati nel trust sono intestati al trustee o ad altro soggetto per conto del trustee;</li></ul>
<ul style=\"margin-top: 0px; margin-bottom: 0px; margin-left: 0px; margin-right: 0px; -qt-list-indent: 1;\"><li style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">il trustee è tenuto ad amministrare, gestire e disporre dei beni in trust secondo indicazioni dettate nell'atto istitutivo del trust e nel rispetto della legge. Il trustee deve rendere conto della gestione. </li></ul>
<p style=\" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;\">Si noti che una proprietà soggetta a trust è una proprietà distinta dalla proprietà che eventualmente precede o segue anche se il proprietario è lo stesso.    </p></OntologyDescription>"^^xsd:string)
