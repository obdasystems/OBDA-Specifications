        </primitiveView>
        <primitiveView>
            <HEAD string="person_info_view(person_id, info_type_id, info, info_name, note)"/>
            <SQLQuery>SELECT p.person_id AS person_id,
       p.info_type_id AS info_type_id,
       p.info AS info,
       i.info AS info_name,
       p.note AS note
